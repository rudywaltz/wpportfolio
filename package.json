{
  "name": "wpportfolio",
  "version": "0.0.1",
  "scripts": {
    "autoprefixer": "postcss style.css --use autoprefixer -o style.css",
    "sass": "npx node-sass --include-path scss ./scss/style.scss ./style.css",
    "sass-watch": "node-sass --watch --include-path scss ./scss/style.scss ./style.css",
    "build": "npm run sass && npm run autoprefixer"
  },
  "devDependencies": {
    "autoprefixer": "9.8.5",
    "node-sass": "4.14.1",
    "postcss-cli": "7.1.1"
  },
  "dependencies": {
    "postcss": "8.4.5"
  },
  "engines": {
    "node": "12.18.0"
  }
}
